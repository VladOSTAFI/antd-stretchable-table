{
  "extends": ["@react-native-community", "airbnb-typescript", "prettier"],
  "parserOptions": {
    "project": ["./tsconfig.json", "./tsconfig-utils.json"]
  },
  "plugins": ["import"],
  "rules": {
    "import/prefer-default-export": 0,
    "@typescript-eslint/no-unused-expressions": ["error", { "allowTaggedTemplates": true }]
  }
}
